                        on_release:
                            app.root.transition = NoTransition()
                            app.root.current = "FeaturesScreen"

                    Widget:
                        size_hint: .06, 1

                    Button:
                        size_hint: .47, 1
                        text: "Go back to Package Selction."
                        font_size: self.height * .52
